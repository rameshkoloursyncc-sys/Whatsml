{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "bitbox/cache": "dev-main",
    "giggsey/libphonenumber-for-php-lite": "^8.13",
    "guzzlehttp/guzzle": "^7.8",
    "inertiajs/inertia-laravel": "^2.0",
    "laravel/breeze": "^2.0",
    "laravel/framework": "^12.0",
    "laravel/pulse": "^1.0",
    "laravel/reverb": "^1.0",
    "laravel/sanctum": "^4.0",
    "laravel/socialite": "^5.11",
    "laravel/tinker": "^2.9",
    "league/flysystem-aws-s3-v3": "*",
    "league/flysystem-ftp": "^3.28",
    "league/omnipay": "^3.2",
    "maatwebsite/excel": "^3.1",
    "moduler/router": "1.1.x-dev",
    "mollie/mollie-api-php": "^2.0",
    "moshabytes/redis": "1.0.1.x-dev",
    "nutgram/nutgram": "^4.17",
    "nwidart/laravel-modules": "^11.0",
    "omnipay/paypal": "^3.0",
    "openai-php/client": "^0.10.1",
    "pbmedia/laravel-ffmpeg": "^8.7",
    "prism-php/prism": "^0.83.2",
    "pusher/pusher-php-server": "^7.2",
    "spatie/laravel-newsletter": "^5.2",
    "spatie/laravel-permission": "^6.3",
    "stripe/stripe-php": "^13.0",
    "tightenco/ziggy": "^2.0",
    "whitecube/laravel-cookie-consent": "^1.2"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.26",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0",
    "phpunit/phpunit": "^11.0",
    "spatie/laravel-ignition": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Modules\\": "modules/"
    },
    "files": [
      "app/Helpers/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
    
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true,
      "wikimedia/composer-merge-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
